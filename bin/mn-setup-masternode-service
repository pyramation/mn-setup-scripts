#!/usr/bin/env python

import sys
from mn_setup_utils.initd import generate_initd, generate_masternode_initd, write_initd_files

print (sys.argv)

if len(sys.argv) != 4:
    raise Exception("wrong number of arguments: daemon_name, daemon (path), username")

daemon_name=sys.argv[1]
daemon=sys.argv[2]
user=sys.argv[3]

write_initd_files(daemon_name, daemon, user)
